[Unit]
Description=HA Calendar Webhook Server
After=network.target

[Service]
Type=simple
User=kenny
WorkingDirectory=/home/kenny/HA-Calendar
ExecStart=/home/kenny/HA-Calendar/venv/bin/python3 /home/kenny/HA-Calendar/src/webhook_server.py
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target
